{
  "name": "redux-breadcrumb-trail",
  "version": "0.1.2",
  "description": "A react component for rendering a Breadcrumb that follows your trail through the application",
  "repository": "https://github.com/MrLeebo/redux-breadcrumb-trail",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel lib --out-dir dist",
    "prepublish": "npm run build",
    "test": "nyc --require babel-core/register --require babel-polyfill mocha -u bdd-lazy-var/rspec --recursive --reporter dot --compilers jsx:babel-core/register --require ./tests/setup.js tests"
  },
  "keywords": [
    "react",
    "redux",
    "router",
    "breadcrumb",
    "trail",
    "stack"
  ],
  "author": "Jeremy Liberman <jliberman@strenuus.com> (http://jeremyliberman.com)",
  "license": "ISC",
  "peerDependencies": {
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "bdd-lazy-var": "^1.2.1",
    "enzyme": "^2.8.0",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "nyc": "^10.2.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "sinon": "^2.1.0"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "standard": {
    "globals": [
      "describe",
      "def",
      "before",
      "beforeEach",
      "it",
      "subject"
    ],
    "ignore": [
      "/dist",
      "/example/node_modules"
    ]
  }
